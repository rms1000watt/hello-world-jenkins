pipeline {
    agent {
        docker {
            image 'docker:latest'
            args '-v /var/run/docker.sock:/var/run/docker.sock'
        }
    }
    // agent none
    stages {
        stage('Build') { 
            steps {
                echo 'Building Done'
            }
        }
        stage('Test') { 
            steps {
                echo 'Testing Done'
            }
        }
    }
}
